---
import Section from "./SectionContainer.astro";
interface Props {
  src: string
}

const { src } = Astro.props
---


<Section class="py-44 pb-12 flex justify-center items-center">
    <img class="rounded-full size-32 border-4 border-black/80 dark:border-white hover:scale-105 transition" src={src} />
</Section>